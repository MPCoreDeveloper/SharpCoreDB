<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <RunConfiguration>
    <TargetFrameworkVersion>net10.0</TargetFrameworkVersion>
    <TargetPlatform>x64</TargetPlatform>
    <TreatNoTestsAsError>false</TreatNoTestsAsError>
    <TestAdaptersPaths>%UserProfile%\.nuget\packages</TestAdaptersPaths>
  </RunConfiguration>
  <xUnit>
    <AppDomain>denied</AppDomain>
    <Parallelization>
      <MaxParallelThreads>0</MaxParallelThreads>
    </Parallelization>
  </xUnit>
</RunSettings>
